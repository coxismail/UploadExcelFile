
@{
    ViewBag.Title = "ExcelUpload";
}
<fieldset class="" ng-app="CustomerImport" ng-controller="CustomerImportController">
    <legend>Upload Customer Excel File</legend>
    <div class="form-inline">
        <input type="file" class="form-control" name="file" accept="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet, application/vnd.ms-excel" onchange="angular.element(this).scope().PrepareThisFile(this.files)" />

        <button type="button" value="Preview" class="btn btn-primary ml-1" ng-disabled="!SelectedFileForUpload" ng-click="ParseExcel() ">

            <div class="spinner-grow spinner-grow-sm text-light" ng-show="showLoader" role="status">
                <span class="sr-only">Loading...</span>
            </div> Preview
        </button>
        <button type="button" value="Insert" class="btn btn-success ml-1" ng-show="ShowPermission" ng-click="InsertData()">

            <div class="spinner-grow spinner-grow-sm text-light" ng-show="showLoaderi" role="status">
                <span class="sr-only">Loading...</span>
            </div> Insert
        </button>

        <span style="color:red">
            {{Message}}
        </span>
        </div>
        <table  class="tableData" ng-show="ShowPermission" id="tableData" width="100%">
            <thead>
                <tr>
                    <th>FirstName </th>
                    <th>LastName</th>
                    <th>EmailID</th>
                    <th>City</th>
                    <th>Country</th>
                </tr>
            </thead>

        </table>

</fieldset>
